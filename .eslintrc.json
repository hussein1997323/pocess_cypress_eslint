{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
        
      "indent": [
        "error",
        2
      ],
      "linebreak-style":0, 
      //[
    //     "error",
    //     "unix"
    //   ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ],
      "no-trailing-spaces": [
        "error"
      ]
    }
  }
  